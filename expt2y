%{
	#include<stdio.h>
	#include<stdlib.h>
	#include<ctype.h>
	int yylex( );
	int yyerror( );
%}
 %token A B


%%
str: S'\n' {printf("valid string\n"); exit(0);}
;
S: A S 
|B
;
%%
int main()
{
	printf("enter the string\n");
	yyparse();
}
int yyerror()
{
	printf("Invalid string\n");
	exit(0);
}
int yywrap()
{
	return 0;
}


