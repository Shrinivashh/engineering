%{
	#include<stdio.h>
	#include "y.tab.h"
%}
%%
a {return A;}
b {return B;}
\n {return yytext[0];}
. {return 0;}
%%
YACC PART

%{
	#include<stdio.h>
	#include<stdlib.h>
	#include<ctype.h>
%}
%%
Str: s\n{printf("Valid string:");exit(0);};
s:A s 
|B
;
%%
int main()
{
	printf("Enter an valid expression:");
	yyparse();
}
int yyerror()
{
	printf("Invalid expression:");
	exit(0);
}
int yywrap()
{
	return 0;
}
