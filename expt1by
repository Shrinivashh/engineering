%{
	#include<stdio.h>
	#include<stdlib.h>
	int yylex(void);
	int yyerror(char* s);

%}

%token NUM;
%left '+' '-'
%left '*' '/'
%left '(' ')'

%%
	expr:e{printf("result:%d\n",$$);return 0;}
	e:e'+'e {$$=$1+$3;}
	|e'-'e {$$=$1-$3;}
	|e'*'e {$$=$1*$3;}
	|e'/'e {$$=$1/$3;}
	|'('e)' {$$=$2;}
	|NUM {$$=$1;}
	;
%%
	int main()
	{
		printf("Enter the arithmatic expression\n");
		yyparse();
		printf("valid expression\n");
		
	}
	int yyerror(char* s)
	{
		printf("\n invalid expression");
		exit(0);
	}
