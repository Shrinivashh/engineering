%{
 	#include<stdio.h>
	#include"y.tab.h"
	extern int yyval;

%}


%%
[ \t] ;
[+|-|*|/|<|>] {printf("\noperator is %s\n",yytext); return OP;}
[0-9]+ {return DIGIT;}
[int|char|bool|float|void|for|do|while|ig|else|return|main] {printf("keyword is:%s\n",yytext);return KEY;}
[a-zA-Z][a-zA-Z0-9]* {printf("identifiers is:%s\n",yytext);return ID;}
. {;}
%%
		
