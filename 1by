%{
	#include<stdio.h>
	#include<stdlib.h>
	int yylex(void);
	int yyerror(char*S);
%}
%token NUM
%left '+' '-'
%left '*' '/'
%left '(' ')'
%%
expr:e {printf("Result:%d\n",$$);return 0;}
	e:e '+' e{$$=$1+$3;}
	|e '-' e{$$=$1-$3;}
	|e '*' e{$$=$1*$3;}
	|e '/' e{$$=$1/$3;}
	|'(' e ')' {$$=$2;}
	|NUM {$$=$1;}
	;
%%
int main()
{
	printf("Enter an expression:");
	yyparse();
	printf("\n valid expression");
}
int yyerror(char *S)
{
	printf("Invalid expression:"):
	exit(0);
}
